{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Streamlit App",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "${workspaceFolder}/src/ui/app.py"
      ],
      "console": "integratedTerminal",
      "env": {
        "PYTHONBREAKPOINT": "pdb.set_trace"
      },
      "justMyCode": false
    }
  ]
}
